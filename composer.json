{
  "name": "arraypress/wp-register-list-filters",
  "version": "1.0.0",
  "description": "Lightweight library for registering custom list table filters in WordPress admin.",
  "homepage": "https://github.com/arraypress/wp-register-list-filters/",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\WP\\RegisterListFilters\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-register-list-filters/issues",
    "source": "https://github.com/arraypress/wp-register-list-filters"
  },
  "keywords": [
    "wordpress",
    "list-filters",
    "admin",
    "posts",
    "users",
    "filters",
    "dropdown"
  ]
}